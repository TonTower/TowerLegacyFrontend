  border-radius: 50px;
  background: rgb(42, 67, 73);
  width: 100%;
  height: 15px;
  position: relative;

  display: flex;
  align-items: center;
  justify-content: center;
}

.scale_text {
  color: rgb(255, 255, 255);
  font-family: Jaro;
  font-size: 10px;
  font-weight: 400;
  line-height: 12px;
  letter-spacing: 0%;

}

.speed {
  width: 36px;
  height: 36px;
  position: absolute;
  top: -7px;
